<?xml version="1.0" encoding="utf-8"?>
<full-backup-content>
    <!-- Include user data -->
    <include domain="sharedpref" path="."/>
    <include domain="database" path="."/>
    <include domain="file" path="."/>

    <!-- Exclude cache and temp files -->
    <exclude domain="cache" path="."/>
    <exclude domain="no_backup" path="."/>
</full-backup-content>